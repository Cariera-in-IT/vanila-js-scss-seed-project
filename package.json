{
  "name": "vanilla-js-scss-seed-project",
  "version": "1.0.0",
  "description": "A project destined to help you get started faster with developing a brand new website using simple technologies like SCSS and JavaScript, and have it ready to deploy on GitHub Pages.",
  "main": "index.js",
  "scripts": {
    "page-url": "node ./webpack.config.js",
    "test2": "npm run test $1",
    "start": "webpack-dev-server --hot=true ",
    "watch": "nodemon --exec \"npm run start\"",
    "build": "rimraf ./docs && webpack",
    "github": "(npm run build) && (git add .) && (git commit -a -m \"No message\" || exit 0) && (git push origin main) && npm run page-url"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "base-href-webpack-plugin": "^3.0.1",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "globule": "^1.3.3",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.6",
    "react-dev-utils": "^12.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2"
  },
  "author": "Cariera în IT: Costică 'Tică' Puntaru & Viorel Mocanu",
  "license": "MIT"
}
